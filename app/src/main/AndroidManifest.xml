<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.finalproject">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CardSearchActivity" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".DecksActivity" />
        <activity android:name=".CardResultActivity" />
        <activity android:name=".CardDetailsActivity" />
        <activity android:name=".DeckListActivity" />
        <activity android:name=".DeckCardDetailActivity" />
        <activity android:name=".NewDeckActivity"></activity>
    </application>

</manifest>